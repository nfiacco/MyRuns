<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<servlet>
		<servlet-name>query</servlet-name>
		<servlet-class>edu.dartmouth.cs.myrunsserver.server.QueryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>query</servlet-name>
		<url-pattern>/query.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>post</servlet-name>
		<servlet-class>edu.dartmouth.cs.myrunsserver.server.PostServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>post</servlet-name>
		<url-pattern>/post.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>delete</servlet-name>
		<servlet-class>edu.dartmouth.cs.myrunsserver.server.DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete.do</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>clear</servlet-name>
		<servlet-class>edu.dartmouth.cs.myrunsserver.server.ClearServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>clear</servlet-name>
		<url-pattern>/clear.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>edu.dartmouth.cs.myrunsserver.server.RegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>query.do</welcome-file>
	</welcome-file-list>

</web-app>